LAYER {
        13-16 {
        NAME: 'aire_protege'
        METADATA {{
              "wms_title" "aire_protege"
              "wms_name"   "aire_protege"
              "wms_include_items" "all"
        }}
        @connection
        @layerconfig
        TYPE: POLYGON
                DATA:"geom from aire_protege using unique fid using srid=4326"
         CLASS {
             STYLE {
              COLOR: 85 255 255
            }
        }
    }
}
