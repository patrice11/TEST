LAYER {
        13-16 {
        NAME: 'lac_mauricie'
        METADATA {{
              "wms_title" "lac_mauricie"
              "wms_name"   "lac_mauricie"
              "wms_include_items" "all"
        }}
        @connection
        @layerconfig
        TYPE: POLYGON
        DATA:"geom from lac_mauricie using unique fid using srid=4326"
        LABELITEM: 'toponyme'
        CLASS {
            STYLE {
                      COLOR: @watercolor
                  OUTLINECOLOR:'#469FC3'                  
             }
            LABEL {
                13-16 {
                	COLOR: @watercolor_lbl
                	OUTLINECOLOR: 255 255 255
                	FONT: 'scb'
                	TYPE: truetype
                	SIZE: 8
                    REPEATDISTANCE: 100
                    MINDISTANCE: 300
                	ANGLE: FOLLOW
                	POSITION: AUTO
                	PARTIALS: FALSE
                }        
            }            
        }
    }
}
