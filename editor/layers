LAYER {
    5-10 {
        NAME: 'rivers'
        TYPE: LINE
        @layerconfig
        DATA {
            5-7: '50m_physical/ne_50m_rivers_lake_centerlines_scale_rank'
            7-10: '10m_physical/ne_10m_rivers_lake_centerlines_scale_rank'	
		}
    	LABELITEM: "name"
        CLASS {
            EXPRESSION: ([strokeweig] >= 0 AND [strokeweig] <= 0.2)
            STYLE {
                COLOR: @rivercolor  
                WIDTH {
					5-6: 1.5
					7-16: 3
				}
            }
            LABEL {
                7-10 {
